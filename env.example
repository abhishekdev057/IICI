# Database Configuration - Replace with your database URL
DATABASE_URL="postgresql://username:password@host:port/database?sslmode=require"
POSTGRES_PRISMA_URL="postgresql://username:password@host:port/database?connect_timeout=15&sslmode=require"

# For uses requiring a connection without pgbouncer
DATABASE_URL_UNPOOLED="postgresql://username:password@host:port/database?sslmode=require"
POSTGRES_URL_NON_POOLING="postgresql://username:password@host:port/database?sslmode=require"

# Parameters for constructing your own connection string
PGHOST="your-database-host"
PGHOST_UNPOOLED="your-database-host"
PGUSER="your-database-username"
PGDATABASE="your-database-name"
PGPASSWORD="your-database-password"

# Parameters for Vercel Postgres Templates
POSTGRES_URL="postgresql://username:password@host:port/database?sslmode=require"
POSTGRES_USER="your-database-username"
POSTGRES_HOST="your-database-host"
POSTGRES_PASSWORD="your-database-password"
POSTGRES_DATABASE="your-database-name"
POSTGRES_URL_NO_SSL="postgresql://username:password@host:port/database"

# Neon Auth environment variables for Next.js (if using Neon)
NEXT_PUBLIC_STACK_PROJECT_ID="your-project-id"
NEXT_PUBLIC_STACK_PUBLISHABLE_CLIENT_KEY="your-publishable-key"
STACK_SECRET_SERVER_KEY="your-secret-key"

# NextAuth Configuration
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-nextauth-secret-key-here-change-in-production"

# Google OAuth (You'll need to set these up in Google Cloud Console)
GOOGLE_CLIENT_ID="your-google-client-id"
GOOGLE_CLIENT_SECRET="your-google-client-secret"

# Application Configuration
NODE_ENV="development"
